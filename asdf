const fs = require("fs")
const path = require("path")

randomTxtFilePath = path.join(__dirname,'res files', "updatedfile.txt")

//callback function
function callback (err, data) {
    if (err) throw err;
    
    textInRandomTxtFile = data.toString()
    console.log(textInRandomTxtFile);
}

//reads file
fs.readFile(randomTxtFilePath, 'utf-8', callback)

